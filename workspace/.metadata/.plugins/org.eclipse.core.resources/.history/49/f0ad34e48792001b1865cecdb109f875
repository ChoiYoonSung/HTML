		<style type="text/css">
			image{
				width: 200px;
			}
		</style>
		<script src="../../js/jquery-3.6.0.js"></script>
		<script type="text/javascript">
			$(document).ready(function(){
				$('p').click(function(){
					var obj = $("<img alt='addImg'>")
					var src = "../../images/" + $(this).attr("title");
					obj.prop("src",src);
					$(this).append(obj);
				})
			})
		</script>
	</head>
