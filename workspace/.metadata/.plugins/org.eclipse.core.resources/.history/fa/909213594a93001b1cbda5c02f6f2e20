			$(document).ready(function(){
// 				$('li:first,:last').css("backgroundColor","yellow");
				$('li').first().css("backgroundColor","yellow");
				$('li').last().css("backgroundColor","yellow");
// 				$('li:eq(5)').css("backgroundColor","red");
				$('li').eq(5).css("backgroundColor","red");
// 				$('li:lt(4)').css("fontWeight","bold");
				$('li').slice(0,4).css("fontWeight","bold");
// 				$('li:gt(3)').css("fontSize","0.5em");
				$('li').slice(3).css("fontSize","0.5em");
				
				//////
				$('tr').even().css("backgroundColor","gray");
				//even - 짝수를 의미, 테이블 0번째 요소부터 
				$('tr').odd().css("backgroundColor","#DDDDDD");
				$('tr').first().css({"backgroundColor" : "black"
									, "color" : "white"});
			})
		</script>
